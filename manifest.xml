<package version="0.0">
  <name>org.openxtalk.library.macfonttools</name>
  <title>FontTools (Mac)</title>
  <author>Paul McClernan</author>
  <version>0.4.0</version>
  <license>community</license>
  <type>library</type>
  <requires name="com.livecode.foreign"/>
  <requires name="com.livecode.objc"/>
  <requires name="com.livecode.arithmetic"/>
  <requires name="com.livecode.binary"/>
  <requires name="com.livecode.list"/>
  <requires name="com.livecode.logic"/>
  <requires name="com.livecode.string"/>
  <requires name="com.livecode.type"/>
  <requires name="com.livecode.engine"/>
  <requires name="com.livecode.typeconvert"/>
  <requires name="com.livecode.array"/>
  <handler name="LogNSObjectClassName" parameters="in optional ObjcId" return="any"/>
  <handler name="ObjC_NSArrayAlloc" parameters="" return="ObjcRetainedId"/>
  <handler name="ObjC_NSArrayComponentsJoinedByString" parameters="in ObjcId,in ObjcId" return="ObjcId"/>
